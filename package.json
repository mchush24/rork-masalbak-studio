  "scripts": {
    "postinstall": "",
    "start": "npx expo start",
    "start:tunnel": "npx expo start --tunnel",
    "start:web": "npx expo start --web",
    "start:web:tunnel": "npx expo start --web --tunnel",
    "dev": "npx expo start",
    "web": "npx expo start --web",
    "backend": "tsx backend/server.ts",
    "backend:watch": "tsx watch backend/server.ts",
    "lint": "expo lint",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backend": "jest --selectProjects backend",
    "test:components": "jest --selectProjects components",
    "test:components:watch": "jest --selectProjects components --watch",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
